{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@redux/*": [
                "src/redux/*"
            ],
            "@reducers/*": [
                "src/redux/reducers/*"
            ],
            "@images/*": [
                "src/images/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@Hooks/*": [
                "src/Hooks/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}